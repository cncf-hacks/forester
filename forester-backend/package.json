{
  "name": "forester-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prestart": "yarn run build",
    "start": "node build/src/index.js",
    "prebuild": "rm -rf build",
    "build": "tsoa spec-and-routes && tsc",
    "predev": "yarn run build",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "lint": "eslint ./src/**/*.ts",
    "spec-and-routes": "tsoa spec-and-routes"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "repository": "git@gitlab.intranet.dvloper.io:forester/forester-backend.git",
  "author": "George-Alexandru Tudurean <atudurean@dvloper.io>",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.8",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "generate-gitignore": "^0.2.6",
    "joi": "^17.12.2",
    "keycloak": "^1.2.0",
    "mongoose": "^8.2.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.13",
    "nodemon": "^3.1.0",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.1.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^6.4.14",
    "@types/swagger-ui-express": "^4.1.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
